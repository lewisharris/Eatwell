{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": [
        {
          "source": "/(.*)",
          "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
        },
        {
          "source": "/vercel_app_domain_name/(.*)",
          "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
        }
      ]
    }
  ]
}
